<resources>

    <!-- Base application theme. -->
    <style name="AppTheme" parent="Theme.AppCompat.NoActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item name="colorAccent">@color/colorAccent</item>
        <item name="android:soundEffectsEnabled">false</item>
    </style>

    <style name="MyTabStyle" parent="TextAppearance.AppCompat.Widget.ActionBar.Title">
        <item name="android:textColor">#CACACA</item>
        <item name="android:textSize">18dp</item>
    </style>
    <style name="MyNavStyle" parent="TextAppearance.AppCompat.Headline">
        <item name="android:textColor">#000</item>
        <item name="android:divider">#aaa</item>
        <item name="android:background">#fff</item>
        <item name="android:textSize">18dp</item>
    </style>

    <style name="MyPopupStyle" parent="Base.V7.Theme.AppCompat.Light">
    </style>


</resources>
